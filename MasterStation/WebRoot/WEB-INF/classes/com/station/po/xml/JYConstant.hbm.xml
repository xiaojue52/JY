<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="com.station.po.JYConstant" table="jy_constant">
	<id name="id" type="java.lang.Integer" column="Id">
   		<generator class="increment">
   		</generator>
 	</id>
  <property name="type" type="string" column="type" length="20"/>
  <property name="key" type="string" column="key" length="10"/>
  <property name="value" type="string" column="value" length="400"/>
 </class>
</hibernate-mapping>