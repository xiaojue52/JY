<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="com.station.po.Gprs" table="Gprs">
	<id name="id" type="java.lang.Integer" column="id">
   	<generator class="increment"/>
 	 </id>
  <property name="number" type="string" column="numb"/>
  <property name="deviceBox" type="string" column="device_box"/>
  <property name="note" type="string" column="note"/>
  <property name="status" type="string" column="status"/>
  <property name="comNumber" type="string" column="comNumber"/>
  <property name="owner" type="string" column="owner"/>
 </class>
</hibernate-mapping>