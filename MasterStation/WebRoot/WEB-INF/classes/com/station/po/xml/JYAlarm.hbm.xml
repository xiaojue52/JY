<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="com.station.po.JYAlarm" table="JY_Alarm">
	<id name="id" column="id">
    <generator class="uuid.hex" />
   </id> 
  <property name="alarmText" type="string" column="alarm_Text" length="400"/>
  <property name="alarmType" type="string" column="alarm_Type" length="400"/>
  <property name="repairUser" type="string" column="repair_User" length="20"/>
  <property name="status" type="string" column="status" length="40"/>
  <property name="alarmDatetime" type="java.sql.Date" column="alarm_Datetime"/>
  <property name="isCabinet" type="java.lang.Integer" column="isCabinet"/>
  <property name="deviceId" type="string" column="device_id" length="40"/>
 </class>
</hibernate-mapping>