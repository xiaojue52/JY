<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="com.station.po.DeviceBox" table="Device_Box">
	<id name="id" type="java.lang.Integer" column="id">
   	<generator class="increment"/>
 	 </id>
  <property name="name" type="string" column="name"/>
  <property name="deviceNumber" type="java.lang.Integer" column="device_number"/>
  <property name="note" type="string" column="note"/>
  <property name="owner" type="string" column="owner"/>
  <property name="identify" type="string" column="identify"/>
  <property name="type" type="string" column="type"/>
  <property name="powerLevel" type="string" column="power_Level"/>
 </class>
</hibernate-mapping>